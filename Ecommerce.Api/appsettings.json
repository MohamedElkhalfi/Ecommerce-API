{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Log4NetConfigFile": "log4net.config",
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "EcoContext": "Data Source=.;Initial Catalog=Ecommerce;Integrated Security=True"
  },
  "Authentication": {
    "Type": "none",
    "Authority": "authorized",
    "Audience": "Audience1",
    "RequireHttpsMetadata": true,
    "ProxyUrl": "http://localhost:5000"
  },
  "Swagger": {
    "uiEndpoint": "/swagger/v1/swagger.json",
    "jsonEndpoint": "/swagger/{0}/swagger.json",
    "title": "ECommerce API",
    "version": "v1"
  },
  "CorsSettings": {
    "AllowedOrigins": [ "http://localhost:4200" ]
  }
}
